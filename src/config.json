                            3
                        ]
                    ]
                },
                "resultlist$table": {
                    "identifier": "table",
                    "fieldsConfiguration": {
                        "idFieldName": "uid",
                        "urlImageTemplate": "http://localhost/~barbet/{acquisition.mission}/{attachments$0$uri}",
                        "urlThumbnailTemplate": "http://localhost/~barbet/{acquisition.mission}/{attachments$0$uri}",
                        "titleFieldName": "acquisition.mission"
                    },
                    "columns": [
                        {
                            "columnName": "uid",
                            "fieldName": "uid",
                            "dataType": "",
                            "process": ""
                        },
                        {
                            "columnName": "Mission",
                            "fieldName": "acquisition.missionName",
                            "dataType": "",
                            "process": ""
                        },
                        {
                            "columnName": "Reception",
                            "fieldName": "transmission.station",
                            "dataType": "",
                            "process": ""
                        },
                        {
                            "columnName": "Archive",
                            "fieldName": "archive.station",
                            "dataType": "",
                            "process": ""
                        },
                        {
                            "columnName": "Date",
                            "fieldName": "enrichment.naturallanguage.search_date_string",
                            "dataType": "",
                            "process": "result.split(':')[0] + ':00:00'"
                        },
                        {
                            "columnName": "Cloud",
                            "fieldName": "enrichment.naturallanguage.search_cloud_string",
                            "dataType": "",
                            "process": "result.split(':')[1]"
                        }
                    ],
                    "process": {
                        "urlImageTemplate": {
                            "process": "v.replace('QUICKLOOK', 'THUMBNAIL').replace('//', '/')"
                        },
                        "urlThumbnailTemplate": {
                            "process": "v.replace('QUICKLOOK', 'THUMBNAIL').replace('//', '/')"
                        }
                    },
                    "details": [
                        {
                            "name": "Technical Informations",
                            "order": 1,
                            "fields": [
                                {
                                    "path": "enrichment.naturallanguage.search_incidence_angle_string",
                                    "label": "Incidence ",
                                    "process": "result.split(':')[1].trim()"
                                },
                                {
                                    "path": "enrichment.naturallanguage.search_quality_string",
                                    "label": "Quality ",
                                    "process": "result.split(':')[1].trim()"
                                }
                            ]
                        },
                        {
                            "name": "Geographical Informations",
                            "order": 2,
                            "fields": [
                                {
                                    "path": "enrichment.geonames.name",
                                    "label": "Country ",
                                    "process": ""
                                },
                                {
                                    "path": "enrichment.geonames.states.name",
                                    "label": "State ",
                                    "process": ""
                                },
                                {
                                    "path": "enrichment.geonames.states.counties.name",
                                    "label": "Counties ",
                                    "process": ""
                                }
                            ]
                        }
                    ],
                    "search_size": 100,
                    "name": "List"
                }
            }
        },
        "server": {
            "server$default": {
                "url": "http://localhost:9999/arlas"
            },
            "collection$default": {
                "collection": "catalog"
            },
            "max_age$default": {
                "max_age": 120
            }
        }
    },
    "effideo": {
        "web": {
            "app": {
                "idFieldName": "uid",
                "components": {
                    "mapbox": {
                        "idFieldName": "uid",
                        "drawtype": "CIRCLE",
                        "basemapUrl": "https://openmaptiles.github.io/osm-bright-gl-style/style-cdn.json",
                        "fontClusterlabel": "Open Sans Bold",
                        "margeExtendPctForLoad": 10,
                        "margeExtendPctForTest": 2,
                        "paintRuleGeoBox": {
                            "line-color": "#FC9F28",
                            "line-opacity": 1
                        },
                        "paintRuleCLusterFeatureFill": {
                            "fill-color": "#56A7EE",
                            "fill-opacity": 0.2
                        },
                        "paintRuleClusterFeatureLine": {
                            "line-color": "#56A7EE",
                            "line-opacity": 1,
                            "line-width": 1
                        },
                        "paintRuleClusterCircle": {
                            "circle-stroke-width": 2,
                            "circle-stroke-color": "#D3D3D3",
                            "circle-color": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        "#f7fcf0"
                                    ],
                                    [
                                        20,
                                        "#e0f3db"
                                    ],
                                    [
                                        30,
                                        "#ccebc5"
                                    ],
                                    [
                                        40,
                                        "#a8ddb5"
                                    ],
                                    [
                                        50,
                                        "#7bccc4"
                                    ],
                                    [
