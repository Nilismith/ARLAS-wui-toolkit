{
    "arlas": {
        "web": {
            "contributors": {
                "powerbars$mission": {
                    "aggregationmodel": [
                        {
                            "type": "term",
                            "field": "acquisition.missionName"
                        }
                    ],
                    "title": "Mission",
                    "identifier": "mission",
                    "name": "Mission"
                },
                "powerbars$reception": {
                    "aggregationmodels": [
                        {
                            "type": "term",
                            "field": "transmission.station"
                        }
                    ],
                    "title": "Reception",
                    "identifier": "reception",
                    "name": "Reception"
                },
                "powerbars$archivage": {
                    "aggregationmodels": [
                        {
                            "type": "term",
                            "field": "archive.station"
                        }
                    ],
                    "title": "Archivage",
                    "identifier": "archivage",
                    "name": "Archivage"
                },
                "swimlane$dropdown": {
                    "identifier": "dropdown",
                    "name": "Swimlane",
                    "swimlanes": [
                        {
                            "id": 1,
                            "name": "Reception",
                            "field": "acquisition.centerViewingDate",
                            "aggregationmodels": [
                                {
                                    "type": "term",
                                    "field": "transmission.station"
                                },
                                {
                                    "type": "datehistogram",
                                    "field": "acquisition.centerViewingDate",
                                    "interval": {
                                        "value": 1,
                                        "unit": "month"
                                    }
                                }
                            ]
                        },
                        {
                            "id": 2,
                            "name": "Mission",
                            "field": "acquisition.centerViewingDate",
                            "aggregationmodels": [
                                {
                                    "type": "term",
                                    "field": "acquisition.missionName"
                                },
                                {
                                    "type": "datehistogram",
                                    "field": "acquisition.centerViewingDate",
                                    "interval": {
                                        "value": 1,
                                        "unit": "month"
                                    }
                                }
                            ]
                        },
                        {
                            "id": 3,
                            "name": "Archivage",
                            "field": "acquisition.centerViewingDate",
                            "aggregationmodels": [
                                {
                                    "type": "term",
                                    "field": "archive.station"
                                },
                                {
                                    "type": "datehistogram",
                                    "field": "acquisition.centerViewingDate",
                                    "interval": {
                                        "value": 1,
                                        "unit": "month"
                                    }
                                }
                            ]
                        }
                    ],
                    "dateunit": "millisecond",
                    "datatype": "time"
                },
                "histogram$timeline": {
                    "identifier": "timeline",
                    "aggregationmodels": [
                        {
                            "type": "datehistogram",
                            "field": "acquisition.centerViewingDate",
                            "interval": {
                                "value": 1,
                                "unit": "week"
                            }
                        }
                    ],
                    "name": "Timeline",
                    "dateunit": "millisecond",
                    "datatype": "time",
                    "isOneDimension":false
                },
                "histogram$cloud": {
                    "identifier": "cloud",
                    "aggregationmodels": [
                        {
                            "type": "histogram",
                            "field": "imageDescription.cloudCoverPercentage",
                            "interval": {
                                "value": 3
                            }
                        }
                    ],
                    "name": "Cloud %",
                    "dateunit": "millisecond",
                    "datatype": "numeric",
                    "isOneDimension": false
                },
                "histogram$quality": {
                    "identifier": "quality",
                    "aggregationmodels": [
                        {
                            "type": "histogram",
                            "field": "target.incidenceAngle",
                            "interval": {
                                "value": 3
                            }
                        }
                    ],
                    "name": "Incidence %",
                    "dateunit": "millisecond",
                    "datatype": "numeric",
                    "isOneDimension": true
                },
                "histogram$azimut": {
                    "identifier": "azimut",
                    "aggregationmodels": [
                        {
                            "type": "histogram",
                            "field": "illumination.azimuthAngle",
                            "interval": {
                                "value": 3
                            }
                        }
                    ],
                    "name": "Azimut",
                    "dateunit": "millisecond",
                    "datatype": "numeric",
                    "isOneDimension": true
                },
                "map$mapbox": {
                    "identifier": "mapbox",
                    "idFieldName": "uid",
                    "drawtype": "CIRCLE",
                    "name": "Map",
                    "initZoom": 2,
                    "geometry": "geometry.geographicBoundingPolygon",
                    "aggregationmodels": [
                        {
                            "type": "geohash",
                            "field": "geometry.centerPoint",
                            "interval": {
                                "value": 1
                            },
                            "withGeoCentroid": true
                        }
                    ],
                    "zoomLevelFullData": 1,
                    "zoomLevelForTestCount": 4,
                    "nbMaxDefautFeatureForCluster": 50,
                    "zoomToNbMaxFeatureForCluster": [
                        [
