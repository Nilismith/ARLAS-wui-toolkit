                                    ],
                                    [
                                        70,
                                        "#2b8cbe"
                                    ],
                                    [
                                        90,
                                        "#3690c0"
                                    ]
                                ]
                            },
                            "circle-radius": {
                                "property": "point_count_normalize",
                                "type": "interval",
                                "stops": [
                                    [
                                        0,
                                        22
                                    ],
                                    [
                                        20,
                                        24
                                    ],
                                    [
                                        30,
                                        26
                                    ],
                                    [
                                        40,
                                        28
                                    ],
                                    [
                                        50,
                                        30
                                    ],
                                    [
                                        60,
                                        32
                                    ],
                                    [
                                        70,
                                        34
                                    ],
                                    [
                                        90,
                                        36
                                    ]
                                ]
                            },
                            "circle-opacity": 1
                        }
                    },
                    "chipssearch": {
                        "search_size": 100,
                        "name": "Search",
                        "autocomplete_field": "relevant_terms",
                        "autocomplete_size": "500"
                    },
                    "processlist": {
                        "url": "process.json"
                    }
                }
            }
        }
    }
}